        tempstring TYPE string.

  SPLIT i_xml_string AT cl_abap_char_utilities=&gt;newline
    INTO TABLE temptable_char.

  CALL METHOD cl_gui_frontend_services=&gt;gui_download
    EXPORTING
      filename = i_fullpath
      filetype = &apos;DAT&apos;
    CHANGING
