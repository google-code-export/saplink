  <method CLSNAME="ZAKE" CMPNAME="GET_FILENAME_FROM_FULLPATH" VERSION="1" LANGU="E" DESCRIPT="Read filename from the full path" EXPOSURE="2" STATE="1" EDITORDER="0 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20100307" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="1" R3RELEASE="702" BCMTDCAT="00" BCMTDSYN="0">
