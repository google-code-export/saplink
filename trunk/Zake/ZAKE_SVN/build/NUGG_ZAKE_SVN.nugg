  <method CLSNAME="ZCL_ZAKE_SVN" CMPNAME="CHECK_SVN_STATUS_AND_ADD_TO_VC" VERSION="1" LANGU="E" DESCRIPT="Check SVN status and add to Version Control" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20100112" CHANGEDBY="BCUSER" CHANGEDON="20100428" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="702" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_ZAKE_SVN" CMPNAME="CHECK_SVN_STATUS_AND_ADD_TO_VC" SCONAME="ZCX_SAPLINK" VERSION="1" LANGU="E" DESCRIPT="SAPlink exception class" MTDTYPE="0" EDITORDER="1 " AUTHOR="BCUSER" CREATEDON="20100112" CHANGEDBY="BCUSER" CHANGEDON="20100428"/>
   <source>method CHECK_SVN_STATUS_AND_ADD_TO_VC.
  DATA command TYPE string.
  DATA command_output	TYPE string_table.
  DATA lo_utils TYPE REF TO lcl_svn_utils.
  DATA lt_path TYPE lcl_svn_utils=&gt;tt_path.
  DATA ls_path TYPE lcl_svn_utils=&gt;ts_path.

  CONCATENATE &apos;status&apos; me-&gt;path
    INTO command SEPARATED BY space.

  command_output = me-&gt;execute_svn_command( command ).

  CONCATENATE &apos;status --xml&apos; me-&gt;path
    INTO command SEPARATED BY space.

  command_output = me-&gt;execute_svn_command( i_command = command i_silent = ABAP_TRUE ).
  create object lo_utils.
  lt_path = lo_utils-&gt;get_paths( command_output ).

  LOOP AT lt_path INTO ls_path WHERE status = &apos;unversioned&apos;.
    me-&gt;add_slinkee_to_versioncontrol( ls_path-path ).
  ENDLOOP.
endmethod.</source>
