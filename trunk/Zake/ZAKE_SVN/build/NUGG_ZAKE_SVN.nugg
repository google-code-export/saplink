CLASS lcl_svn_utils IMPLEMENTATION.
  METHOD get_paths.
    &quot; &apos;svn status --xml&apos; command returns an XML document
    &quot; This XML document will be parsed here to get the paths and its statuses

    DATA lo_xmldocument TYPE REF TO if_ixml_document.
    DATA lv_xml_string TYPE string.
    DATA lo_filter TYPE REF TO if_ixml_node_filter.
    DATA lo_iterator TYPE REF TO if_ixml_node_iterator.
    DATA lo_entry_elem TYPE REF TO if_ixml_element.
    DATA lo_status_elem TYPE REF TO if_ixml_element.
    DATA ls_path TYPE ts_path.
    FIELD-SYMBOLS &lt;fs_status_output&gt; LIKE LINE OF it_status_output.

    IF LINES( it_status_output ) = 0.
      RETURN.
    ENDIF.

    LOOP AT it_status_output ASSIGNING &lt;fs_status_output&gt;.
      CONCATENATE lv_xml_string &lt;fs_status_output&gt; INTO lv_xml_string.
    ENDLOOP.
    lo_xmldocument = zsaplink=&gt;convertstringtoixmldoc( lv_xml_string ).

    lo_filter = lo_xmldocument-&gt;create_filter_name( &apos;entry&apos; ).
    lo_iterator = lo_xmldocument-&gt;create_iterator_filtered( lo_filter ).
    lo_entry_elem ?= lo_iterator-&gt;get_next( ).

    WHILE lo_entry_elem IS BOUND.
      lo_status_elem = lo_entry_elem-&gt;find_from_name( name = &apos;wc-status&apos; ).

      CLEAR ls_path.
      ls_path-path = lo_entry_elem-&gt;get_attribute( name = &apos;path&apos; ).
      IF lo_status_elem IS BOUND.
        ls_path-status = lo_status_elem-&gt;get_attribute( name = &apos;item&apos; ).
      ENDIF.

      APPEND ls_path TO rt_path.

      lo_entry_elem ?= lo_iterator-&gt;get_next( ).
    ENDWHILE.
  ENDMETHOD.                    &quot;get_paths
ENDCLASS.                    &quot;lcl_svn_utils IMPLEMENTATION</localImplementation>
