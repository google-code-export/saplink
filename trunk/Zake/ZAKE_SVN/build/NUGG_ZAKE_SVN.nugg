  <method CLSNAME="ZAKE" CMPNAME="CHECKOUT" VERSION="0" EXPOSURE="0" STATE="0" EDITORDER="0 " DISPID="0 " CREATEDON="00000000" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" BCMTDCAT="00" BCMTDSYN="0">
   <source>method CHECKOUT.
  DATA command TYPE string.
  DATA rev_str TYPE string.

  CONCATENATE &apos;/command:checkout /url:&apos; url &apos; /path:&apos; path
    INTO command.

  IF NOT i_revision IS INITIAL.
    rev_str = i_revision.
    CONCATENATE command &apos; /rev:&apos; rev_str INTO command.
  ENDIF.

  CONCATENATE command &apos;/notempfile /closeonend:1&apos; INTO command SEPARATED BY space.

  me-&gt;execute_svn_command( command ).
endmethod.</source>
   <methodDocumentation/>
  </method>
  <method CLSNAME="ZCL_ZAKE_TORTOISE_SVN" CMPNAME="CHECK_SVN_STATUS_AND_ADD_TO_VC" VERSION="1" LANGU="E" DESCRIPT="Check SVN status and add to Version Control" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="BCUSER" CREATEDON="20100130" CHANGEDBY="BCUSER" CHANGEDON="20100307" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="702" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZCL_ZAKE_TORTOISE_SVN" CMPNAME="CHECK_SVN_STATUS_AND_ADD_TO_VC" SCONAME="ZCX_SAPLINK" VERSION="1" LANGU="E" DESCRIPT="SAPlink exception class" MTDTYPE="0" EDITORDER="1 " AUTHOR="BCUSER" CREATEDON="20100130" CHANGEDBY="BCUSER" CHANGEDON="20100307"/>
   <source>method CHECK_SVN_STATUS_AND_ADD_TO_VC.
  DATA command TYPE string.
  DATA command_output	TYPE string_table.

  CONCATENATE &apos;/command:repostatus /path:&apos; me-&gt;path
    INTO command.
  CONCATENATE command &apos;/notempfile /closeonend:1&apos;
    INTO command SEPARATED BY space.

  command_output[] = me-&gt;execute_svn_command( command ).

endmethod.</source>
